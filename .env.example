# REQUIRED Environment Configuration for InterviewMind
# Copy this file to .env and fill in ALL your API keys
# System will NOT function without these APIs

# REQUIRED AI Service API Keys - No fallbacks, pure AI-powered system
GEMINI_API_KEY=your_gemini_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
GROQ_API_KEY=your_groq_api_key_here

# AI Mode (prod required for full functionality)
AI_MODE=prod

# Flask Configuration
FLASK_ENV=development
SECRET_KEY=your_secret_key_here

# Database Configuration (if using external DB)
# DATABASE_URL=your_database_url_here

# Qdrant Configuration (if using external)
# QDRANT_URL=http://localhost:6333
# QDRANT_API_KEY=your_qdrant_api_key_here